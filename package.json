{
  "name": "quedit",
  "version": "1.0.0",
  "description": "Editing insert SQL statements with ease.",
  "main": "index.js",
  "scripts": {
    "start": "electron-forge start",
    "dev": "concurrently --kill-others \"gulp watch\" \"npm run start\"",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "build": "electron-packager . QuEdit --all--out=./electron-build"
  },
  "author": "dsone",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "alpinejs": "^2.4.1",
    "babelify": "^10.0.0",
    "browserify": "^16.5.1",
    "concurrently": "^5.2.0",
    "electron": "^12.0.0",
    "electron-reload": "^1.5.0",
    "gulp": "^4.0.2",
    "gulp-buffer": "0.0.2",
    "gulp-if": "^3.0.0",
    "gulp-postcss": "^9.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-tap": "^2.0.0",
    "gulp-uglify": "^3.0.2",
    "gulp-uglifycss": "^1.1.0",
    "minimist": "^1.2.5",
    "postcss-custom-properties": "^11.0.0",
    "postcss-import": "^14.0.1",
    "postcss-nested": "^5.0.5",
    "tailwindcss": "^2.0.0"
  },
  "dependencies": {
    "@codemirror/basic-setup": "^0.18.0",
    "@codemirror/gutter": "^0.18.0",
    "@codemirror/state": "^0.18.5",
    "@codemirror/view": "^0.18.6",
    "electron-squirrel-startup": "^1.0.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "quedit"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
